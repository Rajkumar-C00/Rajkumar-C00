{
  "name": "playwright-typescript",
  "version": "1.0.0",
  "description": "Playwright tests using TypeScript",
  "main": "dist/index.js",  // Entry point for your compiled code
  "scripts": {
    "test": "playwright test",  // Command to run Playwright tests
    "build": "tsc",  // Command to compile TypeScript files
    "start": "node dist/index.js"  // Run the compiled code
  },
  "dependencies": {
    "playwright": "^1.22.0",  // Playwright library for browser automation
    "typescript": "^4.5.4"  // TypeScript for transpiling TypeScript code
  },
  "devDependencies": {
    "@types/node": "^16.11.6",  // TypeScript definitions for Node.js
    "@playwright/test": "^1.22.0"  // Playwright test framework (optional, if you're using it)
  },
  "engines": {
    "node": ">=14.0.0"  // Minimum version of Node.js that your project will work with
  },
  "license": "MIT"  // Project license
}
